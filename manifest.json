{
  "name": "Tabs alphabetical sorting",
  "version": "1.0",
  "description": "Sort tabs by alphabetical order based on their title",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/tab_sorter16.png",
      "32": "images/tab_sorter32.png",
      "48": "images/tab_sorter48.png",
      "128": "images/tab_sorter128.png"
    }
  },
  "icons": {
    "16": "images/tab_sorter16.png",
    "32": "images/tab_sorter32.png",
    "48": "images/tab_sorter48.png",
    "128": "images/tab_sorter128.png"
  },
  "manifest_version": 2
}
